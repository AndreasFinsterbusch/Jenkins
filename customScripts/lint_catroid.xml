<?xml version="1.0" encoding="UTF-8"?>
<lint>
    <!-- COMMON Lint warnings -->
    <issue id="ContentDescription" severity="ignore" />
    <issue id="ExtraTranslation" severity="ignore" />
    <issue id="InvalidPackage" severity="ignore" />
    <issue id="MissingTranslation" severity="ignore" />

    <!-- CATROID and CATROIDTEST specific Lint warnings -->
    <!-- Ignore unused strings warnings in languages other than german and english -->
    <issue id="UnusedResources">
        <ignore path="res/values-ja/strings.xml" />
        <ignore path="res/values-ko/strings.xml" />
        <ignore path="res/values-pt/strings.xml" />
        <ignore path="res/values-ro/strings.xml" />
        <ignore path="res/values-ru/strings.xml" />
    </issue>
</lint>
